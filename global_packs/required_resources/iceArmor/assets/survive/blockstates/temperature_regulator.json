{
    "multipart": [
        {   "when": { "facing" : "down" },
            "apply": { "model": "survive:block/empty_regulator", "x": 180 }
        },
		{   "when": { "facing" : "up" },
            "apply": { "model": "survive:block/empty_regulator" }
        },
		{   "when": { "facing" : "north" },
            "apply": { "model": "survive:block/empty_regulator", "x": 90 }
        },
		{   "when": { "facing" : "east" },
            "apply": { "model": "survive:block/empty_regulator", "x": 90, "y": 90 }
        },
		{   "when": { "facing" : "south" },
            "apply": { "model": "survive:block/empty_regulator", "x": 90, "y": 180 }
        },
		{   "when": { "facing" : "west" },
            "apply": { "model": "survive:block/empty_regulator", "x": 90, "y": 270 }
        }
    ]
}
